{
  "version": 2,
  "projects": {
    "mychurch": {
      "projectType": "application",
      "root": "apps/mychurch",
      "sourceRoot": "apps/mychurch/src",
      "prefix": "mychurch",
      "targets": {
        "build": {
          "executor": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/mychurch",
            "index": "apps/mychurch/src/index.html",
            "main": "apps/mychurch/src/main.ts",
            "polyfills": "apps/mychurch/src/polyfills.ts",
            "tsConfig": "apps/mychurch/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/mychurch/src/favicon.ico",
              "apps/mychurch/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/@taiga-ui/icons/src",
                "output": "assets/taiga-ui/icons"
              }
            ],
            "styles": [
              "apps/mychurch/src/styles.scss",
              "node_modules/@taiga-ui/core/styles/taiga-ui-global.less"
            ],
            "scripts": [],
            "customWebpackConfig": { "path": "webpack.config.js" }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mychurch/src/environments/environment.ts",
                  "with": "apps/mychurch/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-builders/custom-webpack:dev-server",
          "options": { "browserTarget": "mychurch:build" },
          "configurations": {
            "production": { "browserTarget": "mychurch:build:production" }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": { "browserTarget": "mychurch:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/mychurch/src/**/*.ts",
              "apps/mychurch/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/mychurch"],
          "options": {
            "jestConfig": "apps/mychurch/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mychurch-e2e": {
      "root": "apps/mychurch-e2e",
      "sourceRoot": "apps/mychurch-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mychurch-e2e/cypress.json",
            "tsConfig": "apps/mychurch-e2e/tsconfig.e2e.json",
            "devServerTarget": "mychurch:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "mychurch:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/mychurch-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "shells-mychurch": {
      "projectType": "library",
      "root": "libs/shells/mychurch",
      "sourceRoot": "libs/shells/mychurch/src",
      "prefix": "tiq-soft-test",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shells/mychurch/src/**/*.ts",
              "libs/shells/mychurch/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shells/mychurch"],
          "options": {
            "jestConfig": "libs/shells/mychurch/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "jumia": {
      "projectType": "application",
      "root": "apps/jumia",
      "sourceRoot": "apps/jumia/src",
      "prefix": "tiq-soft-test",
      "targets": {
        "build": {
          "executor": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/jumia",
            "index": "apps/jumia/src/index.html",
            "main": "apps/jumia/src/main.ts",
            "polyfills": "apps/jumia/src/polyfills.ts",
            "tsConfig": "apps/jumia/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "libs/shells/jumia/src",
                "output": "./"
              },
              {
                "glob": "**/*",
                "input": "libs/shells/jumia/src/lib/jumia-assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@taiga-ui/icons/src",
                "output": "assets/taiga-ui/icons"
              },
              {
                "input": "libs/app-config",
                "glob": "**/*",
                "output": "app-config"
              }
            ],
            "styles": [
              "apps/jumia/src/styles.scss",
              "libs/shells/jumia/src/lib/jumia-styles.scss",
              "node_modules/@taiga-ui/core/styles/taiga-ui-global.less"
            ],
            "scripts": [],
            "customWebpackConfig": { "path": "webpack.config.js" }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "libs/shells/jumia/src/environments/jumia-environment.ts",
                  "with": "libs/shells/jumia/src/environments/jumia-environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-builders/custom-webpack:dev-server",
          "options": { "browserTarget": "jumia:build" },
          "configurations": {
            "production": { "browserTarget": "jumia:build:production" }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": { "browserTarget": "jumia:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/jumia/src/**/*.ts",
              "apps/jumia/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/jumia"],
          "options": {
            "jestConfig": "apps/jumia/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "jumia-e2e": {
      "root": "apps/jumia-e2e",
      "sourceRoot": "apps/jumia-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jumia-e2e/cypress.json",
            "tsConfig": "apps/jumia-e2e/tsconfig.e2e.json",
            "devServerTarget": "jumia:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "jumia:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/jumia-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "shells-jumia": {
      "projectType": "library",
      "root": "libs/shells/jumia",
      "sourceRoot": "libs/shells/jumia/src",
      "prefix": "jumia",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shells/jumia/src/**/*.ts",
              "libs/shells/jumia/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shells/jumia"],
          "options": {
            "jestConfig": "libs/shells/jumia/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "models-jumia": {
      "root": "libs/models/jumia",
      "sourceRoot": "libs/models/jumia/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/models/jumia/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/models/jumia"],
          "options": {
            "jestConfig": "libs/models/jumia/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "models-core": {
      "root": "libs/models/core",
      "sourceRoot": "libs/models/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/models/core/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/models/core"],
          "options": {
            "jestConfig": "libs/models/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/angular" },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": { "style": "scss" }
  },
  "defaultProject": "mychurch"
}
